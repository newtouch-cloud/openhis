    function printDrugUsage(type) {
        var cfmx = $gridCfmx.getDataIDs();
        var lscfmx = [];
        //取全部处方号
        for (var i = 0; i < cfmx.length; i++) {
            var getCfh = $gridCfmx.jqGrid("getRowData", cfmx[i]);
            if (getCfh.yfmc == "口服" || getCfh.yfmc == "外用")
                lscfmx.push(getCfh.cfh);
