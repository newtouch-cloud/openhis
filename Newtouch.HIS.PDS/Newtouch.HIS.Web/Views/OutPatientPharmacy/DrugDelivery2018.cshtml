                },
                { label: '剂量', name: 'jl', align: 'right', width: 60 },
                { label: '剂量单位', name: 'jldw', align: 'left', width: 55 },
                { label: '用法', name: 'yfmc', align: 'left', width: 60 },
                { label: '医生嘱托', name: 'yszt', align: 'left', width: 100 },
                { label: '开立医生', name: 'ysmc', align: 'left', width: 60 },
                { label: '收费时间', name: 'sfsj', align: 'left', width: 100, formatter: "date", formatoptions: { srcformat: 'Y-m-d H:i', newformat: 'Y-m-d H:i' } },
                { label: '药品分组', name: 'czh', align: 'center', width: 70 }
            ]
        });
    }

    //发药
    function newtouch_event_f6() {
        var id = $gridPatients.jqGrid("getGridParam", "selrow");
        if (!id) {
            $.modalAlert("请选中患者信息！", 'warning');
            return;
        }

        $.modalConfirm("确定发药吗？", function (flag) {
            if (!flag) return;
          
            var patient = new Array();
            var cfmx = $gridCfmx.getDataIDs();
            for (var i = 0; i < cfmx.length; i++) {
                var rowdata = $gridCfmx.jqGrid("getRowData", cfmx[i]);
                const rowid = cfmx[i]
                //追溯码
                var zsm = $("#sel_zsm_" + rowid).val()
                //是否拆零
                var sfcl = parseInt($("#sel_sfcl_" + rowid).val())

                // 验证追溯码数量。 是否拆零选是，只需要扫一个，否的话需要扫对应入库数量的码
                const zsmLength = $("#sel_zsm_" + rowid).val().split(',').filter(entry => entry.trim() !== '').length
                var maxZsmLength = 1
                if (sfcl === 1) {
                    maxZsmLength = 1
                } else {
                    maxZsmLength = parseInt(rowdata.sl)
                }
                if (zsmLength != 0 && zsmLength != maxZsmLength) {
                    $.modalMsg("条目" + rowdata.ypmc + "数量与追溯码数量不一致！", "warning", 1500);
                    return false;
                }

                patient.push({
                    "CardNo": $gridPatients.jqGrid('getRowData', id).CardNo,
                    "xm": $gridPatients.jqGrid('getRowData', id).xm,
                    "cfh": rowdata.cfh,
                    "cfnm": rowdata.cfnm,
                    "zsm": zsm,
                    "sfcl":sfcl,
                });
            }
            if (patient.length <= 0) {
                $.modalAlert("未找到有效的发药信息！", 'warning');
                return;
            }
            $.najax({
                type: "POST",
                url: "/OutPatientPharmacy/ExecAllDeliveryDrug",
                dataType: "json",
                data: { patients: patient },
                success: function (res) {
                    if (res.state === "success") {
                        printRpDetailV2(res.data);
                        $.modalMsg('发药成功', 'success', 1000);
                        $gridPatients.trigger("reloadGrid");
                        $gridCfmx.jqGrid("clearGridData");
                        cleanCfInfo();

                    } else {
                        $.modalAlert(res.message, Error);
                    }
                }
            });
            printInfusionCards(0);
            printAtomizationCards(0);
            printDrugUsage(0);
        }
        );
    }
    //全部发药
    function newtouch_event_f7() {
        var pats = new Array();
        var gridPatients = $gridPatients.jqGrid('getRowData_AllLine', null, true);//param2:默认所有行；param3：true返回object
        var cfmx = $gridCfmx.getDataIDs();
        $(gridPatients).each(function () {
			for (var i = 0; i < cfmx.length; i++) {
                var rowdata = $gridCfmx.jqGrid("getRowData", cfmx[i]);
                const rowid = cfmx[i]
                //追溯码
                var zsm = $("#sel_zsm_" + rowid).val()
                //是否拆零
                var sfcl = parseInt($("#sel_sfcl_" + rowid).val())

                // 验证追溯码数量。 是否拆零选是，只需要扫一个，否的话需要扫对应入库数量的码
                const zsmLength = $("#sel_zsm_" + rowid).val().split(',').filter(entry => entry.trim() !== '').length
                var maxZsmLength = 1
                if (sfcl === 1) {
                    maxZsmLength = 1
                } else {
                    maxZsmLength = parseInt(rowdata.sl)
                }
                if (zsmLength != 0 && zsmLength != maxZsmLength) {
                    $.modalMsg("条目" + rowdata.ypmc + "数量与追溯码数量不一致！", "warning", 1500);
                    return false;
                }
				pats.push({
					"CardNo": this.CardNo,
					"xm": this.xm,
					"ShowSfsj": this.ShowSfsj,
					"cfh": rowdata.cfh,
                    "cfnm": rowdata.cfnm,
                    "zsm": rowdata.zsm,
                    "sfcl": rowdata.sfcl,
                });
			}
        });
        if (pats.length <= 0) {
            $.modalAlert("未找到有效的发药信息！", 'warning');
            return;
        }
        $.modalConfirm("确定全部发药吗？", function (flag) {
            if (!flag) return;
            printInfusionCards(0);
            printAtomizationCards(0);
            printDrugUsage(0);
            $.najax({
                type: "POST",
                url: "/OutPatientPharmacy/ExecAllDeliveryDrug",
                data: { patients: pats },
                dataType: "json",
                success: function (res) {
                    if (res.state === "success") {
                        printRpDetailV2(res.data);
                        $.modalMsg('发药成功', 'success', 1000);
                        $gridPatients.trigger("reloadGrid");
                        $gridCfmx.jqGrid("clearGridData");
                        cleanCfInfo();
                    } else  $.modalAlert(res.message, Error);
                },
                error: function (res) {
                    $.modalAlert(res.message, Error);
                }
            });
        });
    }

    function newtouch_event_f8() { printAtomizationCards(1); }
    //打印雾化卡
    function printAtomizationCards(type1) {
        var cfhs = [];
        var cfmx = $gridCfmx.getDataIDs();
        //取全部处方号
        for (var i = 0; i < cfmx.length; i++) {
            var getCfh = $gridCfmx.jqGrid("getRowData", cfmx[i]);
            if (getCfh.yfmc == "雾化")
                cfhs.push(getCfh.cfh);
        }
        if (cfhs.length > 0) {
            var newcfh = $.unique(cfhs);//去重
            var cfh = newcfh.join(","); //数组转字符串逗号分割
            var uri = '@reportUrl' + "?tempCode=1311" + "&orgId=" + '@ViewBag.OrganizeId' + "&cfh=" + cfh + "&mzh=" + tempMzh + "&UserCode=" + opeator.UserCode;
            window.open(uri);
        }
        else { if (type1 == 1) $.modalAlert("无雾化处方，不能打印雾化卡！", 'warning'); }
    }

    function newtouch_event_f9() {
        printInfusionCards(1);
     }
    //打印输液卡
    function printInfusionCards(type1) {
        var cfmx = $gridCfmx.getDataIDs();
        var lscfmx = [];
        //取全部处方号
        for (var i = 0; i < cfmx.length; i++) {
            var getCfh = $gridCfmx.jqGrid("getRowData", cfmx[i]);
            if (getCfh.yfmc == "静脉滴注")
                lscfmx.push(getCfh.cfh);
        }
        if (lscfmx.length > 0) {
            var newcfh = $.unique(lscfmx);//去重
            var cfh = newcfh.join(",");//数组转字符串逗号分割
            //查询含输液的组号
            $.najax({
                type: "POST",
                url: "/OutPatientPharmacy/GetRecipeGroupNum",
                data: { cfh: cfh, type: "静脉滴注" },
                dataType: "text",
                success: function (res) {
