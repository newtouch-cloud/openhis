        if (lscfmx.length>0) {
            var newcfh = $.unique(lscfmx);//去重
            var cfhStr = newcfh.join(",");//数组逗号串连
            var uri = '@reportUrl' + "?tempCode=1306" + "&orgId="+'@ViewBag.OrganizeId' +"&cfh=" + cfhStr;
            window.open(uri);
        } else { if (type == 1) $.modalAlert("无口服、外用药处方，不能打印用法标签！", 'warning'); }
    }
