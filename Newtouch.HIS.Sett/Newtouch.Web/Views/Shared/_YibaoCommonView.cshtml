@{
    //是否和医保交易
    var openYbSett = SysConfigReader.Bool("Outpatient_ChargeFee_OpenYbSett");
    //医保所属地，区分系统将执行何处医保逻辑
    var medicalInsurance = SysConfigReader.String("Outpatient_MedicalInsurance");
}
@if (openYbSett == true && medicalInsurance=="changshu")
{
    <object style="display:none;" id="yibaoAX" classid="clsid:e7f3c5b7-14d2-45b9-973d-ef1d670b79e8" codebase="/dll/ActiveX.cab"></object>
    <script src="@SiteUrl.GetStaticResourceScriptUrl("~/Content/js/yibao.common.js", false)"></script>
}
@if (openYbSett == true && medicalInsurance == "guian")
{
    <object style="display:none;" id="guianyibaoAX" classid="clsid:C9C0DE21-9370-445A-934F-821DE0DD550A" codebase="../../ActiveX/GuianActiveX.cab"></object>
    <script src="@SiteUrl.GetStaticResourceScriptUrl("~/Content/js/guianyibao.common.js", false)"></script>
}