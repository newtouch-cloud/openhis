        if (type === '1201') {
            $('.1201-show').show(); // 显示医药机构信息
            $('.time-show').hide();
            $('.35-show').hide();
            $('.3607-show').hide();
        } else if (type === '3607') {
            $('.3607-show').show();
            $('.1201-show').hide();
            $('.time-show').hide();
            $('.35-show').hide();
            $("#page_num").val(1);
            $("#page_size").val(100);
        } else if (type.startsWith("35")) {
            $('.1201-show').hide(); // 隐藏医药机构信息
            $('.time-show').show();
            $('.35-show').show();
            $('.3607-show').hide();
        } else if (type === "5101") {
            $('.3607-show').hide();
            $('.1201-show').hide();
            $('.time-show').hide();
            $('.35-show').hide();
        } else {
            $.modalAlert("不能识别的类型", 'error');
        }
    }

    $('#sel_jgcxtype').change(function () {
        showTH();
