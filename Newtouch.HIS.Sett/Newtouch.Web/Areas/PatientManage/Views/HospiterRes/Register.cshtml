        if (patModel["gjCode"]) {
            $("#gjmc").val(patModel["gjmc"]).attr('data-label', patModel["gjCode"]);
        }
        if (patModel["mzCode"]) {
            $("#mzmc").val(patModel["mzmc"]).attr('data-label', patModel["mzCode"]);
        }
        GetinpatientInfo(patModel.patid);
    }
