@model Newtouch.HIS.Web.Core.Models.BottomButtonViewModel
@{
    Array.Sort(Model.ShowKeyList);
}
<div class="toolbar" style="float:right;width:80%;margin:10px 30px 20px;text-align:right;">
    @{
        foreach (var i in Model.ShowKeyList)
        {
            @Html.Raw(string.Format("<a class=\"btn btn-primary\" style=\"margin-left:3px;\" id=\"btn_bottombutton_f{0}\" onclick=\"newtouch_globalevent_f{0}()\">", i))
            switch (i)
            {
                case 2:
                    @Html.Raw(string.Format("F{0}：{1}", i, Model.F2Text))
                    break;
                case 3:
                    @Html.Raw(string.Format("F{0}：{1}", i, Model.F3Text))
                    break;
                case 4:
                    @Html.Raw(string.Format("F{0}：{1}", i, Model.F4Text))
                    break;
                case 5:
                    @Html.Raw(string.Format("F{0}：{1}", i, Model.F5Text))
                    break;
                case 6:
                    @Html.Raw(string.Format("F{0}：{1}", i, Model.F6Text))
                    break;
                case 7:
                    @Html.Raw(string.Format("F{0}：{1}", i, Model.F7Text))
                    break;
                case 8:
                    @Html.Raw(string.Format("F{0}：{1}", i, Model.F8Text))
                    break;
                case 9:
                    @Html.Raw(string.Format("F{0}：{1}", i, Model.F9Text))
                    break;
                case 10:
                    @Html.Raw(string.Format("F{0}：{1}", i, Model.F10Text))
                    break;
            }
            @Html.Raw("</a>")
        }
    }
</div>