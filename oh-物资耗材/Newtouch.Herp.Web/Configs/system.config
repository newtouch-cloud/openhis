  <!-- ================== 2：开发系统相关配置 ================== -->
  <!-- 登录身份状态维持时间（单位：分）-->
  <add key="LoginStatusKeepMinute" value="120" />
  <!-- 多终端互踢 -->
  <add key="EnableMultipleLoginLimit" value="true" />
  <!-- 多终端互踢-同一账号，最大同时在线数 -->
  <add key="MultipleLoginLimit_Count" value="10" />
  <!-- Reids缓存的key前缀（为了多应用使用同一个数据库时，防止缓存错乱） -->
  <add key="Reids_Cache_Prefix" value="$newtouch.herp$"/>
  <!-- 应用Id -->
  <add key="AppId" value="newtouch.herp"/>
  <add key="AppName" value="物资耗材系统"/>
  
  
  <!-- 顶级组织机构Id -->
