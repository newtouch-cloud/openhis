  <!-- 创建新用户 时 默认关联角色 -->
  <add key="CreateUser_Default_RoleCode" value="DefaultRole"/>
  <!-- 静态站点的Host -->
  <add key="SiteStaticHost" value="http://localhost:8014/"/>
  <!-- 日志服务的Url -->
  <add key="His.Log.URL" value="http://localhost:9004/"/>
  <!-- 开关-是否加载左侧菜单树（默认加载） -->
  <add key="Is_LoadNavTree" value="true"/>
  <!-- 开关-是否开启菜单页面监控（左侧菜单树，点击，右边页面加载的时间） -->
  <add key="Is_NavPageLoadMoniter" value="true"/>
 
  <!-- 登录时是否显示验证码 -->
  <add key="Is_CheckChkCode" value="false"/>
  
  <!-- ================== 3：业务相关配置 ================== -->
