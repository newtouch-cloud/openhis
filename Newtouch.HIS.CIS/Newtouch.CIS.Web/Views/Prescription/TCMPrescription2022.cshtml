        for (var i = 0; i < rowIds.length; i++) {
            var saveResult = $("#gridzycfnew").saveRow(rowIds[i], null, null, null, function (callbackRowId) { }, null, null);
            //库存检验
            if(isMedicineSearchRelatedKC === 'True'){
                var rowData = $("#gridzycfnew").getRowData(rowIds[i]);
                var thissl = rowData.sl;
                if(thissl){
                    if(!ypkccheck(rowData.zxks,rowData.ypCode,parseInt(thissl))){
                        $.modalAlert(rowData.ypmc + "药品库存不足", "error");
                        saveResult = false;
                    }
                }
            }
            if (!saveResult) {
                EnableTCMnewInlineEditBox();    //重启启用编辑 否则下次Save时会返回false
                return;   //保存失败，则return
            }
        }
        var tieshu = $('#tieshu').val();
        var cfyf = $('#cfyf').val();
        var djbz = $('#djbz').is(':checked');
        if ($('#daijiantieshu').val() == 0 || $('#daijiantieshu').val() == "") {
            djbz = false;
        }
        var djfs = $('#djfs').val();//代煎方式
        var djts = $('#daijiantieshu').val();//代煎付数
        var cfzt = $('#cfzt').val();//嘱托
        if (tieshu == null || tieshu=="") {
            $.modalAlert("请填写付数", "error");
            return;
        }
        if (djts > "0" && djfs=="") {
            $.modalAlert("请选择代煎方式！", "error");
            return;
        }
        var gridzycfnewData = $("#gridzycfnew").jqGrid('getRowData_AllLine', null, true);
        $.each(gridzycfnewData, function () {    //去掉action
            delete this.action;   //去掉action
            delete this.jqRowId;   //去掉jqRowId

            if (savetodb == '1') {
                if (this.ypmc == undefined || this.ypmc == "") {
                    $.modalAlert("缺少名称", "warning");
                    saveResult = false;
                    return false;
                } else if (this.ypgg == undefined || this.ypgg == "") {
                    $.modalAlert("缺少规格", "warning");
                    saveResult = false;
                    return false;
                } else if (isNaN(this.dj) || this.dj == "") {
                    $.modalAlert("缺少单价", "warning");
                    saveResult = false;
                    return false;
                } else if (isNaN(this.mcjl) || this.mcjl == "") {
                    $.modalAlert("缺少剂量", "warning");
                    saveResult = false;
                    return false;
                } else if (this.mcjldw === undefined || this.mcjldw == "") {
                    $.modalAlert("缺少单位", "warning");
                    saveResult = false;
                    return false;
                } else if (isNaN(this.sl) || this.sl == "") {
                    $.modalAlert("缺少数量", "warning");
                    saveResult = false;
                    return false;
                } else if (this.dw === undefined || this.dw == "") {
                    $.modalAlert("缺少单位", "warning");
                    saveResult = false;
                    return false;
                } else if (isNaN(this.je) || this.je == "") {
                    $.modalAlert("缺少金额", "warning");
                    saveResult = false;
                    return false;
                }
                if (this.ypmc2 != undefined && this.ypmc2 != "") {
                    if (this.ypmc2 == undefined || this.ypmc2 == "") {
                        $.modalAlert("缺少名称", "warning");
                        saveResult = false;
                        return false;
                    }
                    else if (this.ypgg2 == undefined || this.ypgg2 == "") {
                        $.modalAlert("缺少规格", "warning");
                        saveResult = false;
                        return false;
                    } else if (isNaN(this.dj2) || this.dj2 == "") {
                        $.modalAlert("缺少单价", "warning");
                        saveResult = false;
                        return false;
                    } else if (isNaN(this.mcjl2) || this.mcjl2 == "") {
                        $.modalAlert("缺少剂量", "warning");
                        saveResult = false;
                        return false;
                    } else if (this.mcjldw2 === undefined || this.mcjldw2 == "") {
                        $.modalAlert("缺少单位", "warning");
                        saveResult = false;
                        return false;
                    } else if (isNaN(this.sl2) || this.sl2 == "") {
                        $.modalAlert("缺少数量", "warning");
                        saveResult = false;
                        return false;
                    } else if (this.dw2 === undefined || this.dw2 == "") {
                        $.modalAlert("缺少单位", "warning");
                        saveResult = false;
                        return false;
                    } else if (isNaN(this.je2) || this.je2 == "") {
                        $.modalAlert("缺少金额", "warning");
                        saveResult = false;
                        return false;
                    }
                }
            }
            this.tieshu = tieshu;
            this.cfyf = cfyf;
            this.djbz = djbz;
            this.sfbz = false;
            this.djfs = djfs;
            this.djts = djts;
            this.cfzt = cfzt;
            var thismxCfId = this.cfId;
            var matchedHisCf = $.jsonWhere(window.alldataArray.zycf, function(icfmx){
                return !!icfmx.cfId && icfmx.cfId == thismxCfId;
            });
            if(matchedHisCf && matchedHisCf.length){
                this.cfzhdysj = matchedHisCf[0].cfzhdysj;
                this.sendtohisResult = matchedHisCf[0].sendtohisResult;
                this.klrq = matchedHisCf[0].klrq;
            }
            this.cftag = $("#tip_jmcf").val();
        });

        if (!saveResult) {
            EnableTCMnewInlineEditBox();    //重启启用编辑 否则下次Save时会返回false
            return;   //保存失败，则return
        }
        //保存数据
        window.alldataArray.zycf = $.jsonWhere(window.alldataArray.zycf, function(icfmx){
            if(!!!icfmx.cfId){
                return false;   //编辑列表里有
            }
            for(var iIndex = 0; iIndex < gridzycfnewData.length; iIndex++){
                if(gridzycfnewData[iIndex].cfh == icfmx.cfh){
                    return false;
                }
            }
            return true;
        });
        $.each(gridzycfnewData, function(){
            window.alldataArray.zycf.push(this);
        });


        zylocaldata = new Array();

        if(!(justUpdateAlldataArray == true)){
            if(savetodb == '1'){
                if (SaveData(false, function(){
                    window.initHistoryPresTree('@Html.Raw(((int)EnumCflx.TCMPres).ToString())');
                    $.modalAlert("保存成功", 'success');
                    var griddata = $("#gridzycfnew").jqGrid('getRowData_AllLine', null, true);
                    $.each($("#gridzycfnew").jqGrid('getRowData_AllLine', null, true),
                        function () { //1.删除行
                            var selRowId = $(this.ypmc).attr('rowid');
                            deleteTCMRowData(selRowId, false);
                        });
                })) {
                    $("#gridzycfnew").clearGridData();
                } else {
                    EnableTCMnewInlineEditBox();
                }
            }
            else{
                //跳回病历页
                $('#myTab [href="#linkbl"]').trigger('click'); //初始化显示哪个tab
                //病例页重新加载处方，处方数据发生了变更
                triggleActive();
            }
        }
        else {
            triggleActive();
        }
    }

    function getTCMActionStr() {
        return "<i class='fa fa-plus-square-o' style='font-size: large; color: #09a3ea;' onclick='addTCMRowData($(this).parent().parent().attr(\"id\"));return false;'></i>&nbsp;&nbsp;&nbsp;<i class='fa fa-minus-square-o' style='font-size: large; color: #09a3ea;' onclick='deleteTCMRowData($(this).parent().parent().attr(\"id\"));return false;'></i>";
    }

    //另存为模板
    function saveAsTCMTemplate(){
        $.modalOpen({
            id: "Form",
            title: "存为模板",
            url: "/MedicalRecord/Form",
            width: "400px",
            height: "250px",
            callBack: function (iframeId) {
                var mbObj = top.frames[iframeId].submitForm();
                if (!(mbObj && mbObj.mbmc && mbObj.mblx)) {
                    return;
                }
                $.modalClose("Form");
                //模板表
                var mbObj = {
                    mblx: mbObj.mblx,
                    cflx: @Html.Raw(((int)EnumCflx.TCMPres).ToString()),
                    mbmc: mbObj.mbmc,
                }
                //获取所有行Id，遍历使编辑框处于保存状态
                var rowIds = $("#gridzycfnew").jqGrid('getDataIDs');
                for (var i = 0; i < rowIds.length; i++) {
                    var saveResult = $("#gridzycfnew").saveRow(rowIds[i], null, null, null, function (callbackRowId) { }, null, null);

                    if (!saveResult) {
                        EnableTCMnewInlineEditBox();    //重启启用编辑 否则下次Save时会返回false
                        return;   //保存失败，则return
                    }
                }
                var gridzycfnewData = $("#gridzycfnew").jqGrid('getRowData_AllLine');
                if(!gridzycfnewData || gridzycfnewData.length==0){
                    $.modalAlert("模板明细不能为空", 'warning');
                    return;
                }

                mbObj.tieshu = $('#tieshu').val();
                mbObj.cfyf = $('#cfyf').val();
                mbObj.djbz = $('#djbz').is(':checked');

                $.each(gridzycfnewData, function () {    //去掉action
                    delete this.action;   //去掉action
                    delete this.jqRowId;   //去掉jqRowId
                });

                $.najax({
                    url: "/TemplateManage/PresTemplate/SaveData",
                    dataType: "json",
                    data: { mbObj: mbObj, mxList: gridzycfnewData },
                    type: "POST",
                    success: function (data) {
                        $.modalAlert("保存成功", 'warning');
                        window.$('#current').trigger('click');
                    },
                    complete:function(){
                        EnableWMInlineEditBox();    //重启启用编辑 否则下次Save时会返回false
                    }
                });

            }
        });
    }

</script>
