    //计算明细里的数量
    function CalculateTCMEachLineSlANDJe(rowid, recalc) {
        var currRowZe = 0.00;

        var tieshu = $.undefinedwith0($('#tieshu').val());    //贴数
        if ($('#' + rowid + '_mcjl').val() == null || $('#' + rowid + '_mcjl').val()=="") {
            $('#' + rowid + '_mcjl').val("1");
        }
        if ($('#' + rowid + '_mcjl2').val() == null || $('#' + rowid + '_mcjl2').val() == "") {
            $('#' + rowid + '_mcjl2').val("1");
        }
        var mcjl = $('#' + rowid + '_mcjl').val();     //剂量
        var mcjl2 = $('#' + rowid + '_mcjl2').val();
        if(tieshu && mcjl){
            $('#' + rowid + '_sl').val(roundingBy4she6ru5chengshuang((parseFloat(tieshu) * parseFloat($.undefinedwith0(mcjl))),0))

            //下面计算明细里的金额
            var currRowSl = $('#' + rowid + '_sl').val(); //数量
            var currRowDj = $('#' + rowid + '_dj').val();

            //前提：中药的单位、剂量单位要维护的一致
            if (currRowDj && parseFloat(currRowSl) > 0) {
                //单价 数量 不为空
                currRowZe = roundingBy4she6ru5chengshuang(parseFloat(currRowDj) * parseFloat(currRowSl),2);
            }

            $('#' + rowid + '_je').val(currRowZe);
        }
        if (tieshu && mcjl2) {
            $('#' + rowid + '_sl2').val(roundingBy4she6ru5chengshuang((parseFloat(tieshu) * parseFloat($.undefinedwith0(mcjl2))), 0))

            //下面计算明细里的金额
            var currRowSl = $('#' + rowid + '_sl2').val(); //数量
            var currRowDj = $('#' + rowid + '_dj2').val();
            var currRowZe2;
            //前提：中药的单位、剂量单位要维护的一致
            if (currRowDj && parseFloat(currRowSl) > 0) {
                //单价 数量 不为空
                currRowZe2 = roundingBy4she6ru5chengshuang(parseFloat(currRowDj) * parseFloat(currRowSl), 2);
            }

            $('#' + rowid + '_je2').val(currRowZe2);
        }
        if(!(recalc === false)){
            //下面计算整个处方的金额
            CalculateTCMPresAmount();
        }
    }

    //计算明细里的金额
    function CalculateTCMEachLineJe(rowid) {
        //下面计算明细里的金额
        var currRowSl = $('#' + rowid + '_sl').val(); //数量
        var currRowDj = $('#' + rowid + '_dj').val();
