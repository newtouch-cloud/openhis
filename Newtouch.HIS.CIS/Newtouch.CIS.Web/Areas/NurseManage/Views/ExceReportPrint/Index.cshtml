        $("#zxdlb").change(function () {
            var zxd = $('#zxdlb').val();
            var yzxz = $('input[name="optionsRadios"]:checked').val();
            if (yzxz == 1 && $('#zxdlb').val() == "yzd") {
                $("#printgoon").show();

            } else {
                $("#printgoon").hide();
            }
            var patcheck = $("#PatTreeList").getCurrentNode();
            if (!!patcheck && patcheck.Ex1 == "c") {
                reloadGrid(zyh);
            }

        });

        $("input[name='optionsRadios']").click(function () {
            var yzxz = $('input[name="optionsRadios"]:checked').val();
            if (yzxz == 1 && $('#zxdlb').val() == "yzd") {
                $("#printgoon").show();

            } else {
