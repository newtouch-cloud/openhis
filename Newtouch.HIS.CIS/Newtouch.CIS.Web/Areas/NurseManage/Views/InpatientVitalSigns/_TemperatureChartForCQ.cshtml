        if (clr % 1 == 0) {
            clr = '#' + clr;    //#333 666之类的
        }
        if (type == 'dotline') {//画虚线
            context.setLineDash([2, 3]);
        }
        context.strokeStyle = clr;//线条颜色
        context.lineWidth = 1;//设置线宽
        context.beginPath();
        context.moveTo(p1.x, p1.y);
        context.lineTo(p2.x, p2.y);
        context.stroke();//画线框
        return true;
    }

