    //删除并解绑医嘱
    function DeleteBind(Id,yzlb) {
        let res = confirm('是否作废/停止该医嘱？');
        if (res == true) {
            var yzxz = "2";
            if (yzlb=="临") {
                yzxz = "1";
            }
            $.ajax({
                url: "/NurseManage/OrderExecution/DeleteBind",
                data: { zyh: currentobj.zyh, yzid: Id, yzxz: yzxz },
                dataType: "json",
                type: "POST",
                async: false,
                success: function (data) {
                    if (data != null && data.message != "") {
                        $.modalAlert("作废/停止失败！" + data.message, 'warning');
                        return;
                    }
                    else {
                        $.modalAlert("作废/停止成功！", 'success');
                        btn_search();
                        return;
                    }
                }
            });
        }
    }
