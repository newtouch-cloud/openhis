@{
    var domain = Newtouch.Core.Common.Utils.ConfigurationHelper.GetAppConfigValue("Document_Domain");
}
@if (!string.IsNullOrWhiteSpace(domain))
{
    <script type="text/javascript">
        document.domain = '@(domain)';
    </script>
}
<script type="text/javascript" src="@SiteUrl.GetStaticResourceScriptUrl("~/js/jquery/jquery.choose.js")"></script>