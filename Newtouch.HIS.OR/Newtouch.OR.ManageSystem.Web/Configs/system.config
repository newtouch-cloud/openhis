  <!-- API地址-统一授权用户身份换取 -->
  <add key="UAUserInfoGetAPI" value="http://localhost:8002/api/UA/GetUserInfo"/>
  <!-- Sett地址配置（统一授权登录配置）（Sett.Web外网地址） -->
  <add key="Sett_UALoginAddress" value="http://localhost:8002/Login/UALogin"/>
  
  <!-- 顶级组织机构Id -->
  <add key="Top_OrganizeId" value="d8ee85f8-d8d1-4b5c-bd9e-1761f44242d8"/>
