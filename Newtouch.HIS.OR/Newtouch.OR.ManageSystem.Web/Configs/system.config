<?xml version="1.0" encoding="utf-8"?>
<appSettings>
  <!-- ================== 1：系统软件参数配置 ================== -->
  <!-- 软件版本 -->
  <add key="Version" value="1.00.00.0" />

  <!-- ================== 2：开发系统相关配置 ================== -->
	<!-- 登录时是否显示验证码 -->
	<add key="Is_CheckChkCode" value="false"/>
  <!-- 登录身份状态维持时间（单位：分）-->
  <add key="LoginStatusKeepMinute" value="3000" />
  <!-- 多终端互踢 -->
  <add key="EnableMultipleLoginLimit" value="true" />
  <!-- 多终端互踢-同一账号，最大同时在线数 -->
  <add key="MultipleLoginLimit_Count" value="10" />
  <!-- Reids缓存的key前缀（为了多应用使用同一个数据库时，防止缓存错乱） -->
  <add key="Reids_Cache_Prefix" value="$OR$"/>
  <!-- 应用Id -->
  <add key="AppId" value="OR"/>
  <add key="AppName" value="新致手术室管理系统"/>

  <!-- 页面domain（嵌套其他域页面，并执行js 要设置成同一域）-->
