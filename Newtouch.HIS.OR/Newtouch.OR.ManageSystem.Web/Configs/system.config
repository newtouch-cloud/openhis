  <add key="CreateUser_Default_RoleCode" value="DefaultRole"/>
  <!-- 静态站点的Host -->
  <add key="SiteStaticHost" value="http://61.172.179.73:41142/"/>
	<!-- 登录时是否显示验证码 -->
	<add key="Is_CheckChkCode" value="true"/>
  <!--<add key="SiteStaticHost" value="localhost:9061"/>-->
  <!-- 日志服务的Url -->
  <add key="His.Log.URL" value="http://59.80.30.157:20106/"/>
