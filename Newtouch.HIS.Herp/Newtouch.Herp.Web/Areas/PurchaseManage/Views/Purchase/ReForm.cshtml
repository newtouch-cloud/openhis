        }
        debugger;
        var par = new Object();
        par["THMX"] = list; //退货明细
        par["CZLX"] = "1";//1 新增 将该条新建立的信息报送市药事系统。 3 作废 将已报送市药事系统的相同编码的信息予以作废
        par["PSDBM"] = $("#psdbm").val();//配送点
        par["yybm"] = "77370296000";//医院编码
