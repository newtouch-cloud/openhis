                { label: '<span class="required">*</span>退货数量', name: 'THSL', width: 100, align: 'center', editable: true, editwidth: "100%" },
                { label: '<span class="required">*</span>退货单价', name: 'THDJ', width: 100, align: 'center', editable: true, editwidth: "100%" },
